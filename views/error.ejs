<% layout('layouts/Boilerplate') %>
<div class="row mt-5">
  <div class="col-md-6 mx-auto text-center">
    <div class="alert alert-danger" role="alert">
      <h4 class="alert-heading"><%= err.message %></h4>
      <% if (process.env.NODE_ENV !== 'production') {%>
      <p><%= err.stack %></p>
      <% } %>
    </div>
  </div>
</div>
